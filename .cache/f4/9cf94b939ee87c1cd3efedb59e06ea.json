play: flex;\r\n  flex-wrap: wrap;\r\n  width: 500px;\r\n  height: 500px;\r\n  outline: 2px solid var(--color-primary);\r\n  outline-offset: -1px;\r\n  border-collapse: collapse;\r\n  border-radius: 10px;\r\n}\r\n\r\n.square {\r\n  border: 1px solid var(--color-secondary);\r\n  width: 166.666666667px;\r\n  height: 166.666666667px;\r\n  border-collapse: collapse;\r\n  cursor: pointer;\r\n  transition: background-color 200ms linear;\r\n  position: relative;\r\n}\r\n.square:hover {\r\n  background-color: var(--color-gray);\r\n}\r\n\r\n/* X symbol styling */\r\n.x {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  background: var(--color-secondary);\r\n  height: 120px;\r\n  width: 5px;\r\n  transition: transform 250ms linear;\r\n}\r\n.x-1 {\r\n  transform: translate(-50%, -50%) rotate(45deg) scale(0);\r\n}\r\n.x-1.shown {\r\n  transform: translate(-50%, -50%) rotate(45deg) scale(1);\r\n}\r\n\r\n.x-2 {\r\n  transform: translate(-50%, -50%) rotate(-45deg) scale(0);\r\n}\r\n.x-2.shown {\r\n  transform: translate(-50%, -50%) rotate(-45deg) scale(1);\r\n}\r\n\r\n/* O symbol styling */\r\n.o {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 50%;\r\n  transform: translate(-50%, -50%) scale(0);\r\n  height: 120px;\r\n  width: 120px;\r\n  border-radius: 50%;\r\n  background: transparent;\r\n  border: 5px solid var(--color-secondary);\r\n  transition: transform 250ms linear;\r\n}\r\n.o.shown {\r\n  transform: translate(-50%, -50%) scale(1);\r\n}\r\n\r\n/* animation to all the figure divs */\r\n.square > div {\r\n  transition: transform 250ms linear;\r\n}\r\n\r\n/* Winning annimation */\r\n@keyframes winning {\r\n  0% {\r\n    opacity: 1;\r\n  }\r\n  25% {\r\n    opacity: 0.1;\r\n  }\r\n\r\n  50% {\r\n    opacity: 1;\r\n  }\r\n\r\n  75% {\r\n    opacity: 0.1;\r\n  }\r\n\r\n  100% {\r\n    opacity: 1;\r\n  }\r\n}\r\n\r\n.animation {\r\n  animation: winning 1000ms linear;\r\n}\r\n\r\n/* styling the icons */\r\n.fa-lightbulb,\r\n.fa-volume-up,\r\n.fa-volume-mute {\r\n  position: absolute;\r\n  top: 1rem;\r\n  left: 1rem;\r\n  cursor: pointer;\r\n  font-size: 1.5rem;\r\n  color: var(--color-secondary);\r\n  cursor: pointer;\r\n  transition: transform 250ms linear;\r\n}\r\n\r\n.fa-lightbulb:hover,\r\n.fa-volume-up:hover,\r\n.fa-volume-mute:hover {\r\n  color: var(--color-dark-gray);\r\n}\r\n\r\n.fa-volume-up,\r\n.fa-volume-mute {\r\n  top: 3.5rem;\r\n}\r\n\r\n.player-count {\r\n  position: relative;\r\n  width: 25px;\r\n  height: 25px;\r\n\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n}\r\n\r\n.fa-user {\r\n  cursor: pointer;\r\n  font-size: 1.5rem;\r\n  color: var(--color-secondary);\r\n  cursor: pointer;\r\n  transition: transform 250ms linear;\r\n}\r\n.user-1 {\r\n  position: absolute;\r\n  top: 0;\r\n  left: 25%;\r\n  transform: translateY(-50%);\r\n}\r\n.user-2 {\r\n  position: absolute;\r\n  top: 50%;\r\n  left: 100%;\r\n  transform: translateY(-50%);\r\n}\r\n\r\n/* styling player stats */\r\naside {\r\n  margin-top: 3rem;\r\n  display: flex;\r\n  gap: 2rem;\r\n  align-items: center;\r\n  justify-content: center;\r\n  flex-wrap: wrap;\r\n}\r\n\r\n.player {\r\n  display: flex;\r\n  flex-direction: column;\r\n  align-items: center;\r\n  justify-content: center;\r\n  gap: 0.5rem;\r\n}\r\n\r\n.player h2 {\r\n  margin: 0;\r\n  text-transform: uppercase;\r\n}\r\n.player small {\r\n  font-size: 1.5rem;\r\n}\r\n\r\n/* dispay-none */\r\n.display-none {\r\n  display: none;\r\n}\r\n\r\n/* media queries!!! */\r\n/* \r\nBreakPoint - 1200px\r\nBreakPoint2 - 450px\r\n*/\r\n\r\n@media only screen and (max-width: 56.25em) {\r\n  .container {\r\n    width: 400px;\r\n    height: 400px;\r\n    outline: 4px solid var(--color-primary);\r\n  }\r\n\r\n  .square {\r\n    width: 133.333333333px;\r\n    height: 133.3333px;\r\n  }\r\n\r\n  .x {\r\n    height: 100px;\r\n  }\r\n\r\n  .o {\r\n    width: 100px;\r\n    height: 100px;\r\n  }\r\n}\r\n\r\n@media only screen and (max-width: 28.125em) {\r\n  .container {\r\n    width: 250px;\r\n    height: 250px;\r\n    outline: 3px solid var(--color-primary);\r\n  }\r\n\r\n  .square {\r\n    width: 83.3333333333px;\r\n    height: 83.3333333333px;\r\n  }\r\n\r\n  .x {\r\n    height: 50px;\r\n  }\r\n\r\n  .o {\r\n    width: 50px;\r\n    height: 50px;\r\n  }\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"10618c6ad82963f55127a21d4ad52f1a","cacheData":{"env":{}}}